<launch>

  <!-- This file makes it easy to include the settings for sensor managers -->  
  
  <!-- Set the file describing which sensor(s) to use to build the octomap  -->
  <arg name="moveit_octomap_sensor_params_file" default="$(find invite_motoman_moveit_config)/config/vision_sensors.yaml" />

  <!--<rosparam command="load" file="$(find invite_motoman_moveit_config)/config/sensors_rs200.yaml" />-->
  
  <!-- Load the robot specific sensor manager; this sets the moveit_sensor_manager ROS parameter -->
  <arg name="moveit_sensor_manager" default="motoman_csda10f" />
  
  <include file="$(find invite_motoman_moveit_config)/launch/$(arg moveit_sensor_manager)_moveit_sensor_manager.launch.xml">
    <arg name="moveit_octomap_sensor_params_file" value="$(arg moveit_octomap_sensor_params_file)"/>
  </include>
  

</launch>
